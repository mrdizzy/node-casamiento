p.casamiento { font-size: 12px; line-height: 1.8em;  }
.decimal { font-size:18px; position:relative;top:-8px;  };

/* Classes
==================================== */
.casamiento_clearfix { clear:both; }

/* How it works 
==================================== */
#how_it_works { width:100%; border-collapse:collapse; }
#how_it_works td {width:50%; padding:10px;border:0; }
#how_it_works h2.casamiento_how_it_works { color:#636667; font: bold italic 16px Georgia; text-align:left;}
p.casamiento_how_it_works { color:#636667; font-size: 12px; text-align:justify; }

/* Tables 
==================================== */
table.casamiento, table.casamiento_related { width:100%; text-align:center; border-collapse:collapse; }
table.casamiento_related td { text-align:center; }
    table.casamiento th { font: bold 11px "Arial"; color: #636667; text-align:center; }
    td.casamiento_price, td.casamiento_quantity { font: normal 24px "Arial"; color: #636667; text-align:center; }
table.casamiento td, table.casamiento th { border:1px dotted grey; padding:10px; } 
table.casamiento_related td { border:0; }

.prevent_table_stretching { width:100%;}

