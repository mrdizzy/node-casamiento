<script>
var monogram = 1;
$('#show_colours').click(function() {
    $('#colour_table').fadeIn(500);
})


var colour = 1;
$('#colour_right').click(function() {
    colour++;
    if(colour > 9) {
        colour = 1;
    }
    $('#colour_' + colour).fadeIn();
    
    $('#colour_' + colour -1).fadeOut();
})

$('#left_arrow').click(function() {
    
    
    if(monogram >1) {
        
    $('#mono_' + monogram).fadeOut();
        monogram--;
    }
})

$('#right_arrow').click(function() {
   
    if(monogram < 11) {
        monogram++;
    }
    $('#mono_' + monogram).fadeIn();
})


</script>