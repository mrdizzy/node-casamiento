<!doctype html>
<html>
  <head>
  
    <meta name="viewport" content="initial-scale=1, user-scalable=no">
    
    <title>Casamiento</title>
   
    <link rel="shortcut icon" href="/favicon.ico" />   
    <link href='http://fonts.googleapis.com/css?family=Josefin+Slab:400,400italic,700italic' rel='stylesheet' type='text/css'>
   
    <link rel="stylesheet" href="/static/css/all.css" />     
  
    <style type="text/css">
        <%- inline_styles %>
    </style>
    
  </head>

  <body>
    <div id="printsvg"></div>
  
    <%- main %>
    
    <div id="inner_page_container">
      <div id="header_wrapper">
        <div id="header_container"><div id="headline_welcome">              
          <img src="/gfx/logo/drms.svg" id="dreams_slogan">
        </div>
        <div id="menu_lines">
          <div style="width:18px;border-color:black;border-style:solid;border-top:1px;"></div>
          <div style="width:18px;border-color:black;border-style:solid;border-top:1px;margin-top:5px;"></div>
          <div style="width:18px;border-color:black;border-style:solid;border-top:1px;margin-top:5px;"></div>
        </div>
        
        <a href="/"><img src="/gfx/logo/casamiento_gry.svg" id="logo"></a>       
          
        <div class="navigation" style="position:relative">
        
          <div id="slogan_container">
            <img src="/gfx/logo/drms.svg" id="dreams_slogan">
          </div>
            
          <div class="nav_link" id="about">
            <div style="padding-top:15px;height:35px;border-left:1px dotted grey;text-align:center;">
              <img src="/gfx/bullets/line_diamond.png" /> About
            </div>
          </div>
            
          <a href="/products">
            <div id="browse" class="browse nav_link">
              <div>
                <img src="/gfx/bullets/line_diamond.png" /> Browse designs
              </div>          
            </div>
          </a>
            
          <div class="nav_link">
            <div style="padding-top:15px;height:35px;text-align:center;">
              <img src="/gfx/bullets/line_diamond.png" />  Contact us
            </div>
          </div>
        </div>
          
        <div style="clear:both;"></div>
      </div>
    </div>
    
    <div id="middle_column"><%- middle %></div>
    <div class="left_column"><%- left %></div>       
    <div id="right_col" class="right_column"><%- right %></div>
   
    <div id="footer">
      <div class="genesis_collection"><img src="/gfx/genesis.svg" style="width:160px"/></div>
      <div class="footer_seperator"><img src="/gfx/three_across.png" style="width:60px"/></div>
      <div class="castello_collection"><img src="/gfx/castello.svg" style="width:250px;" /></div>
    </div>
    
    <div style="clear:both;"></div>
  </body>
  
  <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.1/underscore-min.js"></script>    
  <script src="http://code.jquery.com/jquery-latest.min.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.0.0/backbone-min.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/handlebars.js/1.3.0/handlebars.min.js"></script>    
  <script src="/javascripts/libraries/fastclick.js"></script>    
  <script type="text/javascript" src="/javascripts/libraries/backbone_syphon.js"></script>    
  <script src="/static/javascripts/all.js"></script>   

  <script type="text/javascript">
    $(function() {    
      FastClick.attach(document.body);  
    })
    <%- javascripts %>
  </script>  
</html>