  <body>
    <%- main %>
    <div id="page_container">
      <div id="inner_page_container">
       
          <div id="header_container">
            <a href="/"><img src="http://www.casamiento.co.uk/gfx/logo/cas.png" id="logo"></a>
            
            <div style="position:relative">
          
