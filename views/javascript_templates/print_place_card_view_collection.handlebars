{{#each groups}}    <div class="group">  {{#each .}}    <div class="print_place_card_view" style="width:{{width}}mm;height:{{height}}mm;">		      <img src="/gfx/crop_marks.svg" class="crop_marks_overlay">            <div class="guest" style="border:0;padding:0;margin:0;text-transform:capitalize;font-size:{{font_size}}mm;font-family:{{font}};height:{{guest_height}}mm;margin-top:{{margin_top}}mm;width:{{width}}mm;line-height:{{guest_height}}mm">{{name}}</div>            <img src="" class="place_card_image">    </div>        {{/each}}      <img src="/gfx/logo/casamiento_black.svg" class="cas_print_logo" />  </div>  {{/each}}