<!doctype html>

<style type="text/css">

div,body,h1,h2,h3,h4,h5,h6 { margin:0;padding:0; }
<% if (colours[1]) { %>
  .global_colour_0 .colour_1 { background-color: <%= colours[1] %> }
<% } %>

<% include css_partials/colour_picker.js %>

@font-face { font-family:"TP"; src: url('http://www.casamiento.co.uk/f/t.eot?') format('eot'), url('http://www.casamiento.co.uk/f/t.woff') format('woff'); }
@font-face { font-family:"GaraI"; src: url('http://www.casamiento.co.uk/f/garamonditalic.eot?') format('eot'), url('http://www.casamiento.co.uk/f/garamonditalic.woff') format('woff'); }
@font-face { font-family:"OS"; src: url('http://www.casamiento.co.uk/f/OSReg.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSL.woff') format('woff'); }
@font-face { font-family:"OSReg"; src: url('http://www.casamiento.co.uk/f/OSReg.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSReg.woff') format('woff'); }
@font-face { font-family:"OS Light"; src: url('http://www.casamiento.co.uk/f/OSL.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSL.woff') format('woff'); }
@font-face { font-family:"OS IL"; src: url('http://www.casamiento.co.uk/f/OSLI.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSLI.woff') format('woff'); }
@font-face { font-family:"OS Condensed"; src: url('http://www.casamiento.co.uk/f/OSCB.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSCB.woff') format('woff'); }

h1 {
  text-align:center;
   font:normal 3.5em GaraI, serif;
}
h2 {
  font: normal 2.2em "TP", serif; 
  letter-spacing:10px; 
  margin:0.5em 0 0.5em;
  padding: 0 10px 0 10px;
  text-align:center; 
}

  .place_square h2 { 
    width:70%;
    margin-top:0.5em;
    padding-top:0;
    letter-spacing:0.2em;
    text-align:left;
    padding-left:5px; 
    font-size:1.6em;
  }
  
  a h2 { color: black; }
 
h4 { 
  border-bottom:1px solid black;
 width:75%;
  font:normal 2.2em GaraI, serif;
  margin:0.5em auto 0.5em;
  padding-bottom:0.5em; 
  text-align:center;
}

  #testimonials h4 {
    font-size: 1.2em;
    margin-right:20%;
    text-align:right; 
    border:0;
  }

h5 { 
  font: 0.9em "OS IL", sans-serif; 
  margin:10px auto;
  padding: 0 1em 0 1em;
}

  #testimonials h5 {
    background-image:url('http://www.casamiento.co.uk/gfx/QUOTE.png');
    background-repeat:no-repeat;
    font-size:1.4em;
    margin:auto;
    padding-left:1.75em;
    text-align:left;
    width:75%; 
  }
  
h6 { 
  font: 0.75em "OS Condensed", sans-serif;
  margin-bottom:1em; 
  padding:0.25em 0.25em 0.25em 1em;
  text-align:left;
}

p { 
  padding: 0 0.25em 0.75em 0.25em; 
  font: 0.9em/1.5em "OS", sans-serif;
  margin-bottom:1em;
}
  #intro p { text-align:left; }
    #intro p.first:first-letter { 
      float:left;
      font: normal 5.5em/0.8em TP, serif;
      padding-right:0.05em;
    }
  
a.caz:link, 
a.caz:active,
a.caz:visited { color:<%= colours[0] %>; }

a.related_link:link, 
a.related_link:active, 
a.related_link:visited {
  text-decoration: none;
}
  .buyitnow a { 
    color:#FFF;
    text-decoration:none; 
  }
  
  
  
  
  
  
  
  

#page_container_casamiento { 
  background-color:#fff; 
  margin:0;
  padding:0;  
}

  #left_column { 
    float:left;
    width:50%; 
  }

    #logo { width:202px; margin:5px 0 5px 10px;}
      
    #slogan { 
      width: 180px; 
      display:block; 
      margin-top:10px;
      margin-right:5px;
      float:right;
    }
    
    .slide { position:relative; }
      .slide img,
      .inner_place_square img,
      .inner_place_square_hidden img { 
        position:relative;
        display:block;
        width:100%;
        z-index:6;
      }
      
      .background_container, 
      .inner_place_square div,
      .inner_place_square_hidden div {
        position:absolute;
        width:100%;
        height:100%;
        z-index:0;
      }
      
      .background_container { 
        background-color:<%= colours[0] %>
      }
      
    .shell { 
      background:url('http://www.casamiento.co.uk/gfx/bullets/wiggle.svg') center no-repeat;
      height:17px;
      padding:1em 0 1em;
    }
    
    #intro {
      clear:both;
      width:95%;
      margin:0 auto;
    }
    
    #signature { 
      width:100px;
      display:inline-block;
    }
    .related { 
      border-bottom:1px solid grey; 
      margin-bottom:0.5em;
    }

      #related_0 {
        width:60%;
        float:left; 
      }

      #related_2, 
      #related_5 { 
        width:60%; 
        float:right; 
      }
      
      #related_1, 
      #related_3, 
      #related_4 { 
        width:39%; 
        float:left; 
      }

    .place_square {
      border:1px solid transparent;
    }
      .place_square:hover { 
        border:1px solid grey;   
        cursor:pointer;
        color:#000;
        text-decoration:underline; 
      }
      .place_square img { width:100%;}
      
      .inner_place_square, 
      .inner_place_square_hidden {
        position:relative;
        width:100%;
      }
      .inner_place_square_hidden { display:none; }

      #related_1 h2, 
       #related_3 h2, 
      #related_4 h2 { font-size:1.2em; }

.buyitnow { 
  background-color:<%= colours[0]%>;
  border-radius:5px;
  color:#FFF;
  font:1.4em OS;
  margin: 1em auto 1em;
  padding:1em;
  text-align:center;
  width:65%;
}
  .buyitnow:hover { 
    background-color:#000;
    text-decoration:underline;
  }
  
  .cart_icon { width:30px; }    
    
    
    
    
    
    
  
  #right_column { 
    border-left:1px dotted black;
    margin-left:50%; 
    min-width:180px;
    text-align:center; 
  }

  #name_place_icon { width:155px; }
  
table { 
  border-collapse:collapse;
  width:100%; 
  border-top:1px dotted grey;
  border-bottom:1px dotted grey;
}
 table table td {
  padding:0.25em; 
  text-align:center;
  }

  table.inside_table {
    border:0;
  }
  
  table table tr.last_row td {
  border-bottom:0;
  }
  
  #table_value_format img { width:100px }
  #table_value_texture {
  font: 1em TP;
  letter-spacing:0.1em;
  }
  #table_value_quantity {
    font: 1.6em TP;
    letter-spacing:0.1em;
  }
  .table_key {
    font: 0.75em "OS Condensed", sans-serif;
    border-right:1px dotted grey;
    border-bottom:1px dotted grey;
    text-align:right;
    padding-right:0.5em;
  }
  
  .table_value {
  border-bottom:1px dotted grey;
  
  border-right:1px dotted grey;
  }
  
.spc {
  border-bottom:1px dotted grey;   
  position:relative;
}

  .a_font {
    margin:0 auto;
    width:75%;
    text-align:center;
    padding:1em 1em 0.5em 1em;
    border-bottom:1px solid #000;
  }
  
.texture { 
  margin-top:0.5em;
  width:2.5em;
  height:2.5em;
  border:1px solid black;
  border-radius:0.25em;
  display:inline-block;
}

  #description { padding:1em }


.step { 
  background-color:<%= colours[0] %>;
  border-bottom-left-radius:10px;
  color:#FFF;
  position:absolute;
  top:0;right:0;
  font:0.8em 'OS Light';
  padding:0.25em;
  width:50%;
}

.weight { 
  display:inline-block;
  border: 1px solid #444;
  margin:0.5em 0 0.2em;
  width:75px;
}

  <% var j = 0 %>
  <% for (var k = 0; k < 3; k++) { %>
    <% for (var i = 0; i < 6; i++) { %>
      <% if (document.related[j]) { %>
        <% var card = document.related[j] %>
          #related_<%= j %> .colour_0 { background-color: <%= card.colours[0] %>; }
          #related_<%= j %> .colour_1 { background-color: <%= card.colours[1] %>; }  
        <% j++ %>
      <% } %>
    <% } %>
  <% } %>
  
</style>











