  <script>

  $(function() {
  
    FastClick.attach(document.getElementById('colour_0'));
    <% if (colours[1]) { %>
      FastClick.attach(document.getElementById('colour_1'));
    <% } %>
  })

  var width = $('#page_container_casamiento').width();

  if (width < 700) reducePage();
  if (width < 500) forMobile();
  $('.place_square').mouseover(function() {
    $(".inner_place_square", this).hide();
    $(".inner_place_square_hidden",this).show();
  })

  $('.place_square').mouseout(function() {
    $(".inner_place_square", this).show();
    $(".inner_place_square_hidden",this).hide();
  })
  <% for (var i = 0; i < colours.length; i ++) { %>
    $('#colour_' + <%=i%>).colorPicker({default_color: '<%= colours[i] %>'}).on('dizzy-cp:hoverColor dizzy-cp:click', function(e, colour) {
       <% if (i == 0) { %>
        $('.global_colour_<%= i %>').css("background-color", colour)
        $('.text_colour_<%= i %>').css("color", colour)

        <% } else { %>
          $('.global_colour_0 .colour_<%= i %>').css("background-color", colour)
        <% } %>
      })
    <% } %>
    
  $(window).resize(function() {
    if ($('#page_container_casamiento').width() < 700) reducePage();    
    if ($('#page_container_casamiento').width() < 500) forMobile();
  });
  
  function reducePage() {
    $('h1').attr("style", "font-size:2.5em;");
    
    $('h2').attr("style", "font-size:1.8em;");
    $('h4').attr("style", "font-size:1.6em;");
    $('.related').attr("style", "clear:both;width:100%;float:none");
    $('.quote h6').attr("style", "width:95%;font-size:1.2em;");
    $('.place_square h2').attr("style", "width:80%;font-size:1.4em;");
    $('#logo').attr("style", "display:block;margin:0 auto;float:none;clear:both");
    $('#slogan').attr("style", "margin:0 auto;float:none;clear:both;width:180px;padding-top:0.5em;padding-bottom:0.5em;");
  }
  
  function forMobile() {
    $('#left_column').attr("style", "float:none;width:100%;clear:both");
    $('#right_column').attr("style", "float:none;width:100%;clear:both;margin:0;");
    $('#related_items').appendTo('#right_column');
    $('#mobile_slide').appendTo('#move_to_mobile'); 
  }
</script>