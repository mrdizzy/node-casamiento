<script type="text/javascript">
<% for (var i = 0; i < colours.length; i ++) { %>

  $('#colour_' + <%=i%>).colorPicker({default_color: '<%= colours[i] %>'}).on('dizzy-cp:hoverColor', function(e, colour) {
    <% if(i == 1) { %>      
      $('.slide > div > div:not(.nocolor)').css("background-color", colour)
    <% } else { %> 
      $('.colour_<%= i %>').css("background-color", colour)
      $('.slide').css("background-color", colour)
    <% } %>
  })
<% } %>

$('.place_square').mouseover(function() {
  $(".inner_place_square", this).hide();
  $(".inner_place_square_hidden",this).show();
})
$('.place_square').mouseout(function() {
  $(".inner_place_square", this).show();
  $(".inner_place_square_hidden",this).hide();
})
</script>