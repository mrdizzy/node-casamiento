 <link href='http://fonts.googleapis.com/css?family=Josefin+Slab:400,700italic,400italic' rel='stylesheet' type='text/css'>   
  
<style type="text/css">

div,body,h1,h2,h3,h4,h5,h6 { margin:0;padding:0; }
<% if (colours[1]) { %>
  .global_colour_0 .colour_1 { background-color: <%= colours[1] %> }
<% } %>

<% include css_partials/colour_picker.js %>
@font-face {
  font-family: "TP";
  src: url('http://www.casamiento.co.uk/f/t.eot?') format('eot'), url('http://www.casamiento.co.uk/f/t.woff') format('woff');
}

@font-face {
  font-family: "OS";
  src: url('http://www.casamiento.co.uk/f/OSReg.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSL.woff') format('woff');
}

@font-face {
  font-family: "OSReg";
  src: url('http://www.casamiento.co.uk/f/OSReg.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSReg.woff') format('woff');
}

@font-face {
  font-family: "OS Light";
  src: url('http://www.casamiento.co.uk/f/OSL.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSL.woff') format('woff');
}

@font-face {
  font-family: "OS IL";
  src: url('http://www.casamiento.co.uk/f/OSLI.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSLI.woff') format('woff');
}

@font-face {
  font-family: "OS Condensed";
  src: url('http://www.casamiento.co.uk/f/OSCB.eot?') format('eot'), url('http://www.casamiento.co.uk/f/OSCB.woff') format('woff');
}

h1 {
  text-align: center;
  font: normal 3em 'Josefin Slab Light', sans-serif;
  line-height:1.1em;
}

h2 {
  font: normal 2.2em "TP", serif;
  letter-spacing: 10px;
  margin: 0.5em 0 0.5em;
  padding: 0 10px 0 10px;
  text-align: center;
}

.place_square h2 {
  width: 70%;
  margin-top: 0.5em;
  padding-top: 0;
  letter-spacing: 0.2em;
  text-align: left;
  padding-left: 5px;
  font-size: 1.6em;
}

a h2 {
  color: black;
}

.welcome_box_container {
	text-align: center;
	border-radius: 1.5em;
	background-color: #EEE;
	width:90%;
	margin:0 auto;
	padding:0.25em;
	margin-top:0.5em;
}



h4 {
  border-bottom: 1px solid black;
  width: 75%;
  font: normal 2.2em 'Josefin Slab Light', serif;
  margin: 0.5em auto 0.5em;
  padding-bottom: 0.5em;
  margin-top:0;
  padding-top:0;
  text-align: center;
}

#testimonials h4 {
  font-size: 1.2em;
  margin-right: 20%;
  text-align: right;
  border: 0;
}

h5 {
  font: 0.9em "OS IL", sans-serif;
  margin: 10px auto;
  padding: 0 1em 0 1em;
}

#testimonials h5 {
  background-image: url('http://www.casamiento.co.uk/gfx/QUOTE.png');
  background-repeat: no-repeat;
  font-size: 1.4em;
  margin: auto;
  padding-left: 1.75em;
  text-align: left;
  width: 75%;
}

h6 {
  font: 0.75em "OS Condensed", sans-serif;
  margin-bottom: 1em;
  padding: 1em 0.25em 0.25em 1em;
  text-align: left;
}

p {
  padding: 0 0.25em 0.75em 0.25em;
  font: 0.9em/1.5em "OS", sans-serif;
  margin-bottom: 1em;
}


a.caz:link,
a.caz:active,
a.caz:visited {
  color: <%=colours[0] %>;
}

a.related_link:link,
a.related_link:active,
a.related_link:visited {
  text-decoration: none;
}

.buyitnow a {
  color: #FFF;
  text-decoration: none;
}

#page_container_casamiento {
  background-color: #fff;
  margin: 0;
  padding: 0;
}

#left_column {
  float: left;
  width: 50%;
}

#logo {
  width: 202px;
  margin: 5px 0 5px 10px;
}

#slogan {
  width: 180px;
  display: block;
  margin-top: 10px;
  margin-right: 5px;
  float: right;
}

.slide {
  position: relative;
}

.slide img,
.inner_place_square img,
.inner_place_square_hidden img {
  position: relative;
  display: block;
  width: 100%;
  z-index: 6;
}

.background_container {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 0;
}

.background_container {
  background-color: <%=colours[0] %>
}

.shell {
  background: url('http://www.casamiento.co.uk/gfx/bullets/wiggle.svg') center no-repeat;
  height: 17px;
  padding: 1em 0 1em;
}

#intro {
  clear: both;
  padding: 10px;
  border-top: 1px dotted grey;
  border-bottom: 1px dotted grey;
}

#signature {
  width: 100px;
  display: inline-block;
}

.related {
  border-bottom: 1px solid grey;
  margin-bottom: 0.5em;
}

#related_0,
#related_6,
#related_12 {
  width: 60%;
  float: left;
}

#related_2,
#related_5,
#related_8,
#related_11,
#related_14,
#related_17 {
  width: 60%;
  float: right;
}

#related_1,
#related_3,
#related_4,
#related_7,
#related_9,
#related_10,
#related_13,
#related_15,
#related_16 {
  width: 39%;
  float: left;
}

.place_square {
  border: 1px solid transparent;
}

.place_square:hover {
  border: 1px solid grey;
  cursor: pointer;
  color: #000;
  text-decoration: underline;
}

.place_square img {
  width: 100%;
}

.inner_place_square,
.inner_place_square_hidden {
  position: relative;
  width: 100%;
}

.inner_place_square_hidden {
  display: none;
}

#related_1 h2,
#related_3 h2,
#related_4 h2 {
  font-size: 1.2em;
}

.buyitnow {
  background-color: <%=colours[0]%>;
  border-radius: 5px;
  color: #FFF;
  font: 1.4em OS;
  margin: 1em auto 1em;
  padding: 1em;
  text-align: center;
  width: 65%;
}

.buyitnow:hover {
  background-color: #000;
  text-decoration: underline;
}

.cart_icon {
  width: 35px;
}

#right_column {
  border-left: 1px dotted black;
  margin-left: 50%;
  min-width: 180px;
  text-align: center;
}

#this_auction_is_for {
  text-align: center;
  border-bottom: 1px dotted grey;
  padding-bottom: 1em;
  margin-bottom: 0;
}

#name_place_icon {
  width: 155px;
}

table {
  border-collapse: collapse;
  width: 100%;
  border-top: 1px dotted grey;
  border-bottom: 1px dotted grey;
}

table table td {
  padding: 0.25em;
  text-align: center;
}

table.inside_table {
  border: 0;
}

table table tr.last_row td {
  border-bottom: 0;
}

#table_value_format img {
  width: 100px
}

#table_value_texture {
  font: 1em TP;
  letter-spacing: 0.1em;
}

#table_value_quantity {
  font: 1.6em TP;
  letter-spacing: 0.1em;
}

.table_key {
  font: 0.75em "OS Condensed", sans-serif;
  border-right: 1px dotted grey;
  border-bottom: 1px dotted grey;
  text-align: right;
  padding-right: 0.5em;
}

.table_value {
  border-bottom: 1px dotted grey;
  border-right: 1px dotted grey;
}

.spc {
  border-bottom: 1px dotted grey;
  position: relative;
}

.a_font {
  margin: 0 auto;
  width: 75%;
  text-align: center;
  padding: 1em 1em 0.5em 1em;
  border-bottom: 1px solid #000;
}

.texture {
  margin-top: 0.5em;
  width: 2.5em;
  height: 2.5em;
  border: 1px solid black;
  border-radius: 0.25em;
  display: inline-block;
}

#description h6 {
  font-size: 20px;
  text-align: justify;
  letter-spacing: 0.1em;
  line-height: 150%;
  font-family: "Gara";
}

.step {
  background-color: <%=colours[0] %>;
  border-bottom-left-radius: 10px;
  color: #FFF;
  position: absolute;
  top: 0;
  right: 0;
  font: 0.8em 'OS Light';
  padding: 0.25em;
  width: 50%;
}

.weight {
  display: inline-block;
  border: 1px solid #444;
  margin: 0.5em 0 0.2em;
  width: 75px;
}

 .welcome_box_container h5 {
	font-size: 1.7em;
	font-family: 'Josefin Slab Light';
	border-bottom: 0;
	padding-top: 1em;
	margin-bottom:0;
	padding-bottom:0;
}

.welcome_box_container p {
	margin-bottom: 0.5em;	
	font-family: "OS IL";
	margin-top:0.5em;
	padding-top:0;
	font-size: 1.2em;
	border-top: 0;
	color: #777;
	padding-bottom:0.5em;
}

.welcome_icon_container {
	position:relative;
	padding-bottom:1em;
}

.welcome_icon {
	padding: 0;
	margin: 0;
	display: block;
	position: absolute;
	left: 0;
	right: 0;
	margin-left: auto;
	margin-right: auto;
}

#related_6, #related_12, #related_18 { clear:both; }
  <% var j = 0 %>
  <% for (var k = 0; k < 3; k++) { %>
    <% for (var i = 0; i < 6; i++) { %>
      <% if (document.related[j]) { %>
        <% var card = document.related[j] %>
          #related_<%= j %> .colour_0 { background-color: <%= card.colours[0] %>; }
          #related_<%= j %> .colour_1 { background-color: <%= card.colours[1] %>; }  
        <% j++ %>
      <% } %>
    <% } %>
  <% } %>
  
</style>











<div id="page_container_casamiento">   
<!-- --------------------------------------------------------
  LEFT COLUMN
-->

  <div id="left_column">

    <img src="http://www.casamiento.co.uk/gfx/logo/casamiento_gry.svg" id="logo" />
    <img src="http://www.casamiento.co.uk/gfx/logo/drms.svg" id="slogan" />
      
<!--
  SLIDES 1-3
-->     
    <div class="slide">
    
      <div class="background_container global_colour_0">
        <%- document["background-" + attachments_order[0]]  %>
      </div>
      
      <img src="http://mrdizzy.cloudant.com/test_ebay/<%= _id %>/medium-<%= attachments_order[0]%>" />
    </div>

     
    <h2><%= name %></h2>
    
    <img src="http://www.casamiento.co.uk/gfx/welcomeicons/line.svg" style="display:block;margin:0 auto;width:75%;" class="welcome_line"/>
	
    <div id="description">
      <h6><%= params.description %></h6>
    </div>
 
    <div class="slide">
      
      <div class="background_container global_colour_0">
        <%- document["background-" + attachments_order[1]]  %>
      </div>
      
      <img src="http://mrdizzy.cloudant.com/test_ebay/<%= _id %>/medium-<%= attachments_order[1]%>" />
    </div>


   
  
 <!-- 
    BUY IT BOX 1
 -->
   <% if (!params.buy_it_now) { %>
    <div class="global_colour_0 buyitnow">
      <img src="http://www.casamiento.co.uk/gfx/cart.svg" class="cart_icon"><a href="http://cgi.ebay.co.uk/ws/eBayISAPI.dll?ViewItem&item=<%= params.global_buy_it_now_number ? params.global_buy_it_now_number : buy_it_now_number %>">SEE ALL OF OUR DESIGNS: CLICK HERE FOR A BUY-IT-NOW LISTING WITH MANY MORE OPTIONS AND DESIGNS&hellip;</a>      
    </div>
   <% } %>




 <!-- 
    BLURB 
 -->

  <div id="intro">
    
    <h1>Place cards delivered to you in 2 working days in any colour and font of your choice with guest names printed...</h1>
  
    <img src="http://www.casamiento.co.uk/gfx/welcomeicons/line.svg" style="display:block;margin:0 auto;width:75%;padding-top:1em;padding-bottom:1em;" class="welcome_line"/>
      
    <div id="box_1" class="welcome_box_container">
  
     <div class="welcome_icon_container">
        <img src="http://www.casamiento.co.uk/gfx/welcomeicons/colour.svg"  class="welcome_icon" />
      </div>
      
      <h5>Customise colours to match your theme</h5>
      <p>Every design can be changed to any colour of your choice&mdash;just click or hover over the colour swatch. Note the name of the colour and tell us!</p>
     
    </div>
     
     
     
     
    <div id="box_2"  class="welcome_box_container">
      
    
      <div class="welcome_icon_container">
        <img src="http://www.casamiento.co.uk/gfx/welcomeicons/font.svg"  class="welcome_icon" />
      </div>
     
      <h5>Fonts to match<br/>your mood</h5>
      <p>Choose a typeface to match the mood of your theme&mdash;from clever and corporate to fun and frenetic! </p>
       
     </div>
     
     
    <div id="box_5"  class="welcome_box_container">
  
     <div class="welcome_icon_container">
        <img src="http://www.casamiento.co.uk/gfx/welcomeicons/delivery.svg" class="welcome_icon" />
      </div>
      
      <h5>Delivery within 2 days&hellip;</h5>
      <p>Send us your list of names, choice of colour and fonts, and your place cards will be printed despatched the same day</p>
     
    </div>

  
    <div  id="box_6" class="welcome_box_container">
  
    <div class="welcome_icon_container">
      <img src="http://www.casamiento.co.uk/gfx/welcomeicons/editing.svg" class="welcome_icon" />
    </div>
    
      <h5>Send us your guest names easily&hellip;</h5>
      <p>Just email a list of names or send us an eBay message with them&hellip;</p>
     
    </div>
     
           
    <div  id="box_4" class="welcome_box_container">
  
      <div class="welcome_icon_container">
        <img src="http://www.casamiento.co.uk/gfx/welcomeicons/designs.svg"  class="welcome_icon" />
      </div>
      
      <h5>100s of designs to match your theme</h5>
      <p>Whether it's 80s retro, Hallowe'en, Christmas or traditional Victorian vintage, <a href="http://www.ebay.co.uk/sch/casamiento-wedding-stationery/m.html">browse our designs</a> and find a design to suit! Click on some of the related designs below to have a look at more...</p>
     
     </div>
     
     
  
  </div>
  
  

  
  
<!-- 
  RELATED ITEMS 
  --> 
  <div id="related_items">
    
    <h6>You will also like&hellip;</h6>
        
    <% j = 0 %>

    <% for (var k = 0; k < 3; k++) { %>
      <% for (var i = 0; i < 6; i++) { %>
        <% if (related[j]) { %>
        <div id="related_<%=j %>" class="related">
        
          <a class="caz related_link casamiento" href="http://cgi.ebay.co.uk/ws/eBayISAPI.dll?ViewItem&item=<%= params.global_buy_it_now_number ? params.global_buy_it_now_number : related[j].buy_it_now_number %>">
            <div class="place_square">
            
                <div class="inner_place_square">
                  <div class="background_container colour_0"><%- related[j]["background-" + related[j].attachments_order[0]]  %></div>              
                  <img src="http://mrdizzy.cloudant.com/test_ebay/<%= related[j]._id %>/display-<%= related[j].attachments_order[0]%>" />
                </div>
              
                <div class="inner_place_square_hidden">    
                  <div class="background_container colour_0"><%- related[j]["background-" + related[j].attachments_order[2]]  %></div>
                  <img src="http://mrdizzy.cloudant.com/test_ebay/<%= related[j]._id %>/display-<%= related[j].attachments_order[2]%>" />
                </div>
             
              <h2 class="caz"><%= related[j].name %></h2>
              
            </div>
          </a>
          
        </div>
        <% } %>
        <% j++ %>
        
      <% } %>
    <% } %>

  </div>

</div>   















<!-- -------------------------------------------------------------
  RIGHT COLUMN
  -->

<div id="right_column"> 
  
    <h6 id="this_auction_is_for">This <%= params.buy_it_now ? "buy-it-now listing" : "auction" %> is for:</h6>

    <img src="http://www.casamiento.co.uk/gfx/specs/name_place.svg" id="name_place_icon">
    
    <table>
      <tr>
        <td>
          <table class="inside_table">
          <% if (!params.buy_it_now) { %>
            <tr>
              <td class="table_key" id="table_key_quantity">Quantity:</td>
            
              <td class="table_value" id="table_value_quantity">
                <img src="http://www.casamiento.co.uk/gfx/qty.png" style="display:inline-block;width:40px;">
                <%= params.quantity %>
              </td>
            </tr>
          <% } %>
            
            <tr>
              <td class="table_key" id="table_key_format">Format:</td>              
              <td class="table_value" id="table_value_format"><img src="http://www.casamiento.co.uk/gfx/specs/tent.png"></td>
            </tr>
            
            <tr>
              <td class="table_key" id="table_key_thickness">Card thickness:</td>
              <td class="table_value" id="table_value_thickness">
              
                <div style="padding:1em;">
                  <div class="weight" style="border-top:3px solid #000;"></div> 
                  <span style="font:normal 0.8em OS;">200gsm</span>
                </div>
                
                <div style="padding:1em;padding-top:0;">
                  <div class="weight" style="border-top:7px solid #000;"></div> 
                  <span style="font:normal 0.8em OS;">300gsm</span>
                </div>
              </td>
            </tr>
            
            <tr class="last_row">
              <td class="table_key" id="table_key_texture">Card texture:</td>
              <td class="table_value" id="table_value_texture">
                <div class="texture"></div><br/>Smooth<br/>White
              </td>
            </tr>
            
          </table>
        </td>
        
        <td style="width:35%;">
        
        <% if (params.buy_it_now) { %>
          <div style="font: normal 0.75em 'OS';text-align:center;letter-spacing:0.1em;">PRICES STARTING FROM</div>
          <h2 class="price_and_quantity" style="text-align:center;font-size:2em;margin:0;padding:0;">&pound;<%= params.buy_it_now_price %></h2>
          
        <% } else { %>
        <div style="font: normal 0.75em 'OS';text-align:center;letter-spacing:0.2em;">BIDDING STARTS AT</div>
          <h2 class="price_and_quantity" style="text-align:center;font-size:2em;margin:0;padding:0;">&pound;<%= params.bidding %></h2>
          
<div style="font: normal 0.75em 'OS';margin-top:2em;text-align:center;letter-spacing:0.2em;">OR BUY-IT-NOW FROM</div>

          <h2 class="caz casamiento" style="text-align:center;font-size:2em;margin:0;padding:0;">&pound;<%= params.buy_it_now_price %></h2>
          <p style="font-family:OS;font-size:0.8em;margin:0.5em;">To see more of our beautiful designs with other options and different quantities, see our <a class="caz casamiento text_colour_0" href="http://cgi.ebay.co.uk/ws/eBayISAPI.dll?ViewItem&item=<%= params.global_buy_it_now_number ?  params.global_buy_it_now_number : buy_it_now_number %>">buy-it-now</a> listing</p>
        <% } %>

        </td>
        
      </tr>
    </table>







<div id="move_to_mobile"></div>



  <div class="spc">
<h6>Choose your colour</h6>
      <div class="step global_colour_0">STEP 1</div>
    
    <!-- COLOUR SWATCH PICKER -->
    
    <% for(var i=0; i < colours.length; i++) { %>
      <div id="colour_<%=i %>"></div>
    <% } %>
    <h5>Choose from hundreds of colours: hover over or tap the colour shown above to bring up the hues panel, then hover or tap the colour swatches. Move backwards and forwards through the hues panel by clicking the left &larr; and right &rarr; arrows. Note the name of the colour you would like and let us know in your message.
    </h5>
    
  </div>

 <!-- 
    FONTS 
 -->
  <div class="spc">
    <h6>Choose your font</h6>
    <div class="step global_colour_0">STEP 2</div>

    <h5>Choose a font to suit your event. From small and simple to big and bold, fun and fierce to silent and steely&hellip; choose the font to express the personality you want to convey.</h5>
    
   <% fonts.forEach(function(font) { %>
		
		<div class="a_font">
			<img src="http://www.casamiento.co.uk/fonts/<%= font %>.svg" />
		</div>
		
		<% }) %>
  </div>

  
 <!-- 
    THICKNESS 
 -->
 
