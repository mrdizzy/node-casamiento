        <% } else { %>
          $('.global_colour_0 .colour_<%= i %>').css("background-color", colour)
        <% } %>
      })
    <% } %>
    
  $(function() {
    FastClick.attach(document.getElementById('colour_0'));
    <% if (colours[1]) { %>
      FastClick.attach(document.getElementById('colour_1'));
    <% } %>
  })

  $('.place_square').mouseover(function() {
    $(".inner_place_square", this).hide();
    $(".inner_place_square_hidden",this).show();
  })

  $('.place_square').mouseout(function() {
    $(".inner_place_square", this).show();
    $(".inner_place_square_hidden",this).hide();
  })
  var width = $('#page_container_casamiento').width();

  if (width < 700) reducePage();
  if (width < 500) forMobile();

  $(window).resize(function() {
    if (width < 700) reducePage();
  });
  
  function reducePage() {
    $('h1').attr("style", "font-size:2.5em;");
    
    $('h2').attr("style", "font-size:1.8em;");
