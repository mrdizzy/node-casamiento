<% documents.forEach(function(doc) { -%>
<% if (doc.name == "Cahoot") {
var deposit_account = "12101";
}
else {
var deposit_account = "12160" 
} -%>
<%= deposit_account + "\t" -%>
<%= doc._id + "\t" -%>
<% date = new Date(doc.date); -%>
<% date = date.getDate() + '/' + (date.getMonth() + 1) + '/' + date.getFullYear(); -%>
<%= date + "\t" -%>
<%= "\t" /* Inclusive */ -%>
<%= doc.name + "\t" -%>
<%= "\t" /* First Name */ -%>
<%= "\t" /* Addr 1 - Line 1 */ -%>
<%= "\t" /* Addr 1 - Line 2 */ -%>
<%= "\t" /* Addr 1 - Line 3 */ -%>
<%= "\t" /* Addr 1 - Line 4 */ -%>
<%= doc.name + "\t" /* Memo */ -%>
<%= "\t" /* Allocation Account Number - MUST LEAVE BLANK */ -%>
<%= "£" + doc.amount + "\t\r\n" /* Amount - MUST BE PREFIXED WITH POUND SIGN followed by newline seperators /r/n */ -%>
<%= "\t" /* Cheque Account */ -%>
<%= doc._id + "\t" /* Cheque No. */ -%>
<%= date + "\t" /* Date */ -%>
<%= "\t" /* Inclusive */ -%>
<%= doc.name + "\t" -%>
<%= "\t" /* First Name */ -%>
<%= "\t" /* Addr 1 - Line 1 */ -%>
<%= "\t" /* Addr 1 - Line 2 */ -%>
<%= "\t" /* Addr 1 - Line 3 */ -%>
<%= "\t" /* Addr 1 - Line 4 */ -%>
<%= doc.name + "\t" /* Memo */ -%>
<%= doc.account + "\t" -%>
<%= "£" + doc.amount + "\t\r\n\r\n" -%>
<% }); %>